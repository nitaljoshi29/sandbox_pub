<!-- Start of hiding email and name on webwidget -->
<script type="text/javascript">
    zE('webWidget:on', 'open', function() { //on click of web widget
       var style = document.createElement('style'); //create style element

          style.innerHTML = `

            #Embed > div > div > div > form > div > div.sc-kaNhvL.iIpHXN > div:nth-child(1) {  
                display: none;     
              }
            #Embed > div > div > div > form > div > div.sc-kaNhvL.iIpHXN > div:nth-child(2) {   
                display: none;     
             }
                         `;

      parent.document.getElementById('webWidget').contentDocument.head.appendChild(style);
    });    
</script>
<!-- End  of hiding email and name on webwidget -->  

<nav class="sub-nav">
  <div class="container">
    {{breadcrumbs}}
  </div>
</nav>
  <div class="article-container" id="article-container">
    <article class="gs-article parallax">
      <header class="gs-article-header parallax__layer parallax__layer--back">
        <div class="container">
          <h2 class="gs-article-sub-title">{{dc 'get_started_title'}}</h2>
          <h1 title="{{article.title}}" class="gs-article-title">
            {{article.title}}
            {{#if article.internal}}
            <span class="icon-lock" title="{{t 'internal'}}"></span>
          {{/if}}
        </h1>
          <a  class="button-secondary not-your-product" href="/home">{{dc "not_your_product"}}</a>
       </div>
      </header>
  <section class="article-info parallax__layer parallax__layer--base">
    <div class="container">
      <div class="gs-article-content">
        <div class="article-body">{{article.body}}</div>
      </div>
    </div>
  </section>
<footer>
  <div class="gs-user-manual-block">
    <div class="gs-user-manual-img">
      <img src="{{asset 'user-manuals.png'}}">
    </div>
    <div class="gs-user-manual-text">
      <h3 class="gs-user-manual-title">{{dc "user_manual"}}
    	</h3>
      <p class="gs-user-manual-description">{{dc "user_manual_tagline"}}
    	</p>
    </div>  
    <a class="gs-user-manual-download button" href="download.tomtom.com/something">{{dc "download"}}
    </a>
  </div>
</footer>
