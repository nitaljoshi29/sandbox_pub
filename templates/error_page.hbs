       var style = document.createElement('style'); //create style element

          style.innerHTML = `

            #Embed > div > div > div > form > div > div.sc-kaNhvL.iIpHXN > div:nth-child(1) {  
                display: none;     
              }
            #Embed > div > div > div > form > div > div.sc-kaNhvL.iIpHXN > div:nth-child(2) {   
                display: none;     
             }
                         `;

      parent.document.getElementById('webWidget').contentDocument.head.appendChild(style);
    });    
</script>
